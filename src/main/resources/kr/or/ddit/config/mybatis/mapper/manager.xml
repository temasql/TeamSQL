<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="manager">

	<!-- Insert -->
	<insert id="insert" parameterType="managerVo">
		INSERT INTO MANAGER
		(
			...
		)
		VALUES
		(
			...
		)
	</insert>
	
	<!-- SelectOne -->
	<select id="get" parameterType="String" resultType="managerVo">
		SELECT *
		FROM MANAGER
		WHERE ID = #{id}
	</select>
	
	<!-- SelectList -->
	<select id="list" resultType="managerVo">
		SELECT *
		FROM MANAGER
	</select>
	
	<select id="map" parameterType="map" resultType="managerVo">
		SELECT *
		FROM MANAGER
		WHERE ID = #{map1}
		AND .. = #{map2}
		...
	</select>
	
	<!-- update -->
	<update id="update" parameterType="managerVo">
		UPDATE MANAGER
		SET COLUMN = #{..}
		WHERE ID = #{id}
	</update>
	
	<!-- delete -->
	<delete id="delete" parameterType="String">
		DELETE
		FROM MANAGER
		WHERE ID = #{id}
	</delete>

</mapper>